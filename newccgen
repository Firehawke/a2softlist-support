			;;
		*"disk 2 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 2 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 3 side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 3 Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 3 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 3 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 4 side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 4 Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 4 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 4 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 5 side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 5 Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 5 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 5 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 6 side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 6 Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 6 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 6 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 7 side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 7 Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 7 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 7 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 8 side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 8 Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 8 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 8 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 9 side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 9 Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 9 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 9 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 10 side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 10 Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 10 side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 10 Side B - "/>' >> ../xml/cc/cc$1.xml
			;;

			# Disk Side-Number.
			# e.g. "reading workshop - running free (4am crack) disk a-1.dsk"
			*"disk a-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk A-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk a-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk A-2"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk b-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk B-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk b-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk B-2"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk c-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk C-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk c-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk C-2"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk d-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk D-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk d-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk D-2"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk e-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk E-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk e-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk E-2"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk f-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk F-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk f-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk F-2"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk g-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk G-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk g-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk G-2"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk h-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk H-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk h-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk H-2"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk i-1"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk I-1"/>' >> ../xml/cc/cc$1.xml
			;;
			*"disk i-2"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk I-2"/>' >> ../xml/cc/cc$1.xml
			;;

		# Disk NumberSide.
		# e.g. "voyage of the mimi (4am crack) disk 1a.dsk"
		*"disk 1a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 1 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 1b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 1 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 2a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 2 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 2b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 2 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 3a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 3 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 3b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 3 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 4a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 4 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 4b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 4 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 5a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 5 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 5b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 5 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 6a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 6 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 6b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 6 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 7a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 7 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 7b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 7 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 8a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 8 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 8b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 8 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 9a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 9 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 9b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 9 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 10a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 10 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 10b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 10 Side B"/>' >> ../xml/cc/cc$1.xml
			;;

		# Because case is a fallthrough, it only ever gets here if it fails to match
		# one of the above entries.

		# "Side X - Title" type
		# e.g. "the bard's tale ii (4am and san inc crack) side a - program.dsk"
		*"side a -"*)
			echo -e '\t\t\t<feature name="part_id" value="Side A - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"side b -"*)
			echo -e '\t\t\t<feature name="part_id" value="Side B - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"side c -"*)
			echo -e '\t\t\t<feature name="part_id" value="Side C - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"side d -"*)
			echo -e '\t\t\t<feature name="part_id" value="Side D - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"side e -"*)
			echo -e '\t\t\t<feature name="part_id" value="Side E - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"side f -"*)
			echo -e '\t\t\t<feature name="part_id" value="Side F - "/>' >> ../xml/cc/cc$1.xml
			;;

		# "Side X" type
		# e.g. "reading and me (4am crack) side a.dsk"
		*"side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"side c."*)
			echo -e '\t\t\t<feature name="part_id" value="Side C"/>' >> ../xml/cc/cc$1.xml
			;;
		*"side d."*)
			echo -e '\t\t\t<feature name="part_id" value="Side D"/>' >> ../xml/cc/cc$1.xml
			;;
		*"side e."*)
			echo -e '\t\t\t<feature name="part_id" value="Side E"/>' >> ../xml/cc/cc$1.xml
			;;
		*"side f."*)
			echo -e '\t\t\t<feature name="part_id" value="Side F"/>' >> ../xml/cc/cc$1.xml
			;;
		*"side 1."*)
			echo -e '\t\t\t<feature name="part_id" value="Side 1"/>' >> ../xml/cc/cc$1.xml
			;;
		*"side 2."*)
			echo -e '\t\t\t<feature name="part_id" value="Side 2"/>' >> ../xml/cc/cc$1.xml
			;;

		# Special cases. Will add as they come up.
		*"side 2 (boot)."*)
			echo -e '\t\t\t<feature name="part_id" value="Side 2 - Boot"/>' >> ../xml/cc/cc$1.xml
			;;
	esac

	# Give us the actual floppy definition, including size.
	echo -e -n '\t\t\t<dataarea name="flop" size="' >> ../xml/cc/cc$1.xml
	wc "$filename" | awk '{print $3}' | tr -d '\n' >> ../xml/cc/cc$1.xml
	echo -e '">' >> ../xml/cc/cc$1.xml
	echo -e -n '\t\t\t\t<rom name="' >> ../xml/cc/cc$1.xml
	# BUT! Give us the lowercase filename in the XML definition.
	echo -e -n $lfilename >> ../xml/cc/cc$1.xml
	# As always, tools use case of what's there.
	echo -e -n '" size="' >> ../xml/cc/cc$1.xml
	wc "$filename" | awk '{print $3}' | tr -d '\n' >> ../xml/cc/cc$1.xml
	echo -e -n '" crc="' >> ../xml/cc/cc$1.xml
	crc32 $filename | awk '{print $1}' | tr -d '\n' >> ../xml/cc/cc$1.xml
	echo -e -n '" sha1="' >> ../xml/cc/cc$1.xml
	sha1sum $filename | awk '{print $1}' | tr -d '\n' >> ../xml/cc/cc$1.xml
	echo -e '" />' >> ../xml/cc/cc$1.xml
	echo -e '\t\t\t</dataarea>' >> ../xml/cc/cc$1.xml
	echo -e '\t\t</part>' >> ../xml/cc/cc$1.xml
	((disknum++))
done
echo -e '\t</software>\n' >> ../xml/cc/cc$1.xml
# Change crack tags to cleanly cracked.
sed -i 's/(4am crack)<\/description>/(cleanly cracked)<\/description>/g' ../xml/cc/cc$1.xml
sed -i 's/(san inc crack)<\/description>/(cleanly cracked)<\/description>/g' ../xml/cc/cc$1.xml
sed -i 's/(4am and san inc crack)<\/description>/(cleanly cracked)<\/description>/g' ../xml/cc/cc$1.xml
sed -i 's/ 800K / (800K 3.5") /g' ../xml/cc/cc$1.xml
# If the temp2 file above says this already exists in our MAME softlists,
# then all this work was a waste and we need to delete the XML now.
if [[ -s dupecheck ]]; then
	echo -e "Duplicated entry. Removing generated XML..."
	rm ../xml/cc/cc$1.xml
	echo ..
	echo -e "------"
	exit 1
fi
# Migrate all non-duplicated .dsk and .woz images to the postsorted folder for later Clrmamepro
# parsing so we can be 100% sure the XML is correct even after mame -valid
mv *.dsk ../postsorted 2> /dev/null
mv *.2mg ../postsorted 2> /dev/null
mv *.woz ../postsorted 2> /dev/null
cd ..
echo -e "------"
IFS=$SAVEIFS
