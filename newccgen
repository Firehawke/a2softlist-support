#!/bin/bash
SAVEIFS=$IFS
IFS=$(echo -en "\n\b")
# Remove and recreate the work directories so they're clean.
rm -rf newccoutput 2> /dev/null
rm -rf newccstaging 2> /dev/null
mkdir xml 2> /dev/null
mkdir xml/cc 2> /dev/null
mkdir postsorted 2> /dev/null
mkdir newccoutput 2> /dev/null
mkdir newccstaging 2> /dev/null
# Log the entry for work tracking.
echo -e $1 >> newcclog.txt
# Do the actual search and download.
echo -e "Beginning work on entry [$1].."
ia search '@4am subject:"crack"' --parameters="page=$1&rows=1" --sort="publicdate desc" -i > currentitems.txt
ia download -i --no-directories --glob '*.xml|*.dsk|*.2mg|*.po|*.bin|*.woz|*crack).zip|*.BIN' --destdir="./newccstaging" --itemlist=currentitems.txt
# Now let's decompress any ZIP files we got, forced lowercase names.
unzip -n -qq -LL -j "newccstaging/*.zip" -d newccoutput 2> /dev/null
# Before we go ANY further, let's clean up the .bin files into .2mg ...
cd newccstaging
# THIS use of rename is, unfortunately, Debian-specific. If you're using a
# different distro, you're going to have to modify this.
rename 's/.bin/.2mg/' *.bin 2> /dev/null
cd ..
# Move the meta XML and the disk images to the output folder for processing.
mv newccstaging/*.dsk newccoutput 2> /dev/null
mv newccstaging/*.po newccoutput 2> /dev/null
mv newccstaging/*.2mg newccoutput 2> /dev/null
cp newccstaging/*meta.xml newccoutput 2> /dev/null
cd newccoutput
# Remove stuff we don't want. We don't want to parse the playable.dsk because
# that's an exact copy of the properly named disk. We don't want pictures and
# videos in this case either.
rm 00playable.dsk 2> /dev/null
rm 00playable.2mg 2> /dev/null
rm playable.dsk 2> /dev/null
rm playable.2mg 2> /dev/null
rm 00playable2.dsk 2> /dev/null
rm *fastdsk\ rip\* 2> /dev/null
rm *.a2r 2> /dev/null
rm *.png 2> /dev/null
rm *.mp4 2> /dev/null
rm *.jpg 2> /dev/null
# These next two files seem to pop up a lot with MP4 files, which we don't want.
rm ProjFileList.xml 2> /dev/null
rm project.xml 2> /dev/null
# 4AM sometimes leaves his work disk in the package. We don't want to keep that.
rm *work\ disk* 2> /dev/null
rm *demuffin\'d* 2> /dev/null
# Remove any existing XML file with the same number so we start clean.
rm ../xml/cc/cc$1.xml 2> /dev/null
#Now we start parsing each XML, of which there GENERALLY should only be one.
for filename in *.xml; do
	[ -e "$filename" ] || continue
	# We'll have to handcraft the shortname ourselves.
	echo -e '\t<software name="namehere">' >> ../xml/cc/cc$1.xml
	echo -e -n '\t\t<description>' >> ../xml/cc/cc$1.xml
	xmllint --xpath 'metadata/title/text()' "$filename" | tr -d '\n' >> ../xml/cc/cc$1.xml
	echo -e '</description>' >> ../xml/cc/cc$1.xml
	echo -e -n '\t\t<year>' >> ../xml/cc/cc$1.xml
	xmllint --xpath 'metadata/description/text()' $filename | grep -o '19[0123456789][0123456789]' | tr -d '\n' >> ../xml/cc/cc$1.xml
	echo -e '</year>' >> ../xml/cc/cc$1.xml
	echo -e -n '\t\t<publisher>' >> ../xml/cc/cc$1.xml
	xmllint --xpath 'metadata/description/text()' $filename | grep -o -a -i -F -f ../publishers.txt | tr -d '\n' | sed -e 's/distributed by //g' | sed -e 's/published by //g' | sed -e 's/\&amp;amp;/and/g' >> ../xml/cc/cc$1.xml
	echo -e '</publisher>' >> ../xml/cc/cc$1.xml
	echo -e -n '\t\t<info name="release" value="' >> ../xml/cc/cc$1.xml
	xmllint --xpath 'metadata/publicdate/text()' $filename | awk '{print $1}' | tr -d '\n' >> ../xml/cc/cc$1.xml
	echo -e '"/>' >> ../xml/cc/cc$1.xml
	echo -e -n '\t\t<!--' >> ../xml/cc/cc$1.xml
	xmllint --xpath 'metadata/description/text()' $filename | tr -d '\n' >> ../xml/cc/cc$1.xml
	echo -e -n '-->\n' >> ../xml/cc/cc$1.xml
done
# Now we start working on the disk images. Here's where things get a Little
# hairy. We need both the proper (possibly bad)-cased filename for tools, but
# we need a forced lowercase for the XML.
# Clrmamepro will rename files automagically so the the files are fine as-is;
# we won't be renaming.
disknum=1
for filename in *.po *.dsk *.2mg; do
	[ -e "$filename" ] || continue
	# Here we're generating a forced lowercase version of the name which we'll
	# use in some places in the XML.
	lfilename=$(echo "$filename" | tr '[:upper:]' '[:lower:]' )
	echo -e "Generating disk $disknum with file $lfilename.."
	# Critical: Check for SHA1 dupes.
	# Generate the SHA1 and put it in temp.
	sha1sum $filename | awk '{print $1}' > temp
	# Show on-screen if we got a dupe.
	#grep -a -i -F -n -R -f temp /mnt/c/msys64/src/mame/hash/apple2_flop*.xml
	# If we got a dupe, put it in temp2, otherwise leave a 0-byte file.
	grep -a -i -F -n -R -f temp /mnt/c/msys64/src/mame/hash/apple2_flop*.xml > temp2
	if [[ -s temp2 ]]; then
		echo "dupe" > dupecheck
	fi
	# Start outputting disk information.
	echo -e -n '\t\t<part name="flop' >> ../xml/cc/cc$1.xml
	echo -e -n $disknum >> ../xml/cc/cc$1.xml
	echo -e -n '" interface="' >> ../xml/cc/cc$1.xml

	# Now, is this a 5.25" or 3.5"?
	# In the case of a .po it could technicaly be either, but...
	case $lfilename in
		*".po"*)
			echo -e 'floppy_3_5">' >> ../xml/cc/cc$1.xml
			;;
		*".dsk"*)
			echo -e 'floppy_5_25">' >> ../xml/cc/cc$1.xml
			;;
		*".2mg"*)
			echo -e 'floppy_3_5">' >> ../xml/cc/cc$1.xml
			;;
		*".woz"*)
			# WOZ can be either. We need to pull the data from the WOZ itself.
			# According to the WOZ 2.0 spec, certain info is always hard-set
			# location-wise to help lower-end emulators.
			# The disk type should ALWAYS be at offset 21, and
			# should be "1" for 5.25" and "2" for 3.5" disks.
			disktype=$((16#$(xxd -e -p -l1 -s 21 "$filename")))
			case $disktype in
				*"2"*)
					echo -e 'floppy_3_5">' >> ../xml/cc/cc$1.xml
					;;
				*)
					echo -e 'floppy_5_25">' >> ../xml/cc/cc$1.xml
					;;
			esac
			;;
	esac

	# Generate side/disk number information.

	case $lfilename in

		# "Disk X - Title" type
		# e.g. "swordthrust (4am crack) disk 1 - the king's testing ground.dsk"
		*"disk 1 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 1 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 2 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 2 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 3 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 3 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 4 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 4 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 5 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 5 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 6 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 6 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 7 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 7 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 8 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 8 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 9 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 9 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 10 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 10 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 11 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 11 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 12 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 12 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 13 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 13 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 14 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 14 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 15 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 15 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 16 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 16 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 17 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 17 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 18 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 18 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 19 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 19 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 20 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 20 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 21 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 21 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 22 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 22 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 23 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 23 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 24 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 24 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 25 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 25 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 26 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 26 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 27 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 27 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 28 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 28 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 29 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 29 - "/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 30 -"*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 30 - "/>' >> ../xml/cc/cc$1.xml
			;;

		# "Disk X" type
		# e.g. "read and spell - in the days of knights and castles (4am crack) disk 1.dsk"
		*"disk 1."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 1"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 2."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 2"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 3."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 3"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 4."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 4"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 5."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 5"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 6."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 6"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 7."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 7"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 8."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 8"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 9."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 9"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 10."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 10"/>' >> ../xml/cc/cc$1.xml
			;;

		# "Disk X Side X" type
		# e.g. "read n roll (4am crack) disk 2 side a.dsk"
		*"disk 1 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 1 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 1 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 1 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 2 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 2 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 2 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 2 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 3 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 3 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 3 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 3 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 4 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 4 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 4 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 4 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 5 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 5 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 5 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 5 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 6 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 6 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 6 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 6 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 7 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 7 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 7 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 7 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 8 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 8 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 8 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 8 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 9 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 9 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 9 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 9 Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 10 side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 10 Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk 10 side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk 10 Side B"/>' >> ../xml/cc/cc$1.xml
			;;

		# "Disk X" type
		*"disk a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk c."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk C"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk d."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk D"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk e."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk E"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk f."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk F"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk g."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk G"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk h."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk H"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk i."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk I"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk j."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk J"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk k."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk K"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk l."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk L"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk m."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk M"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk n."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk N"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk o."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk O"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk p."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk P"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk q."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk Q"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk r."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk R"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk s."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk S"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk t."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk T"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk u."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk U"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk v."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk V"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk w."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk W"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk x."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk X"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk y."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk Y"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk z."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk Z"/>' >> ../xml/cc/cc$1.xml
			;;

		# "Disk X Side A/B" type
		*"disk a side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk A Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk a side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk A Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk b side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk B Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk b side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk B Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk c side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk C Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk c side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk C Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk d side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk D Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk d side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk D Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk e side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk E Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk e side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk E Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk f side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk F Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk f side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk F Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk g side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk G Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk g side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk G Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk h side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk H Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk h side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk H Side B"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk i side a."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk I Side A"/>' >> ../xml/cc/cc$1.xml
			;;
		*"disk i side b."*)
			echo -e '\t\t\t<feature name="part_id" value="Disk I Side B"/>' >> ../xml/cc/cc$1.xml
